{
    "swagger": "2.0",
    "info": {
        "version": "0.0.1",
        "title": "Apollo Portal Backend"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "tags": [{
        "name": "config-controller",
        "description": "Config Controller"
    }, {
        "name": "event-controller",
        "description": "Event Controller"
    }, {
        "name": "login-controller",
        "description": "Login Controller"
    }],
    "consumes": ["application/json"],
    "produces": ["application/json"],
    "paths": {
        "/api/config": {
            "get": {
                "tags": ["config-controller"],
                "summary": "config",
                "operationId": "configUsingGET",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/ApplicationProperties"
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                },
                "deprecated": false
            }
        },
        "/api/stream-sse": {
            "get": {
                "tags": ["event-controller"],
                "summary": "streamEvents",
                "operationId": "streamEventsUsingGET",
                "produces": ["text/event-stream"],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/FluxOfServerSentEventOfstring"
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                },
                "deprecated": false
            }
        },
        "/login": {
            "post": {
                "tags": ["login-controller"],
                "summary": "authenticate",
                "operationId": "authenticateUsingPOST",
                "produces": ["application/stream+json"],
                "parameters": [{
                    "in": "body",
                    "name": "authentication",
                    "description": "authentication",
                    "required": true,
                    "schema": {
                        "$ref": "#/definitions/Authentication"
                    }
                }],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/MonoOfTokenCredential"
                        }
                    },
                    "201": {
                        "description": "Created"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                },
                "deprecated": false
            }
        }
    },
    "securityDefinitions": {
        "oauth2": {
            "type": "oauth2",
            "tokenUrl": "http://apollo:8080/auth",
            "flow": "password",
            "scopes": {
                "Apoollo": "Apoollo API"
            }
        }
    },
    "definitions": {
        "ApplicationProperties": {
            "type": "object",
            "properties": {
                "streamSse": {
                    "type": "string"
                }
            },
            "title": "ApplicationProperties"
        },
        "Authentication": {
            "type": "object",
            "properties": {
                "password": {
                    "type": "string"
                },
                "useranme": {
                    "type": "string"
                }
            },
            "title": "Authentication"
        },
        "FluxOfServerSentEventOfstring": {
            "type": "object",
            "properties": {
                "prefetch": {
                    "type": "integer",
                    "format": "int32"
                }
            },
            "title": "FluxOfServerSentEventOfstring"
        },
        "MonoOfTokenCredential": {
            "type": "object",
            "title": "MonoOfTokenCredential"
        },
        "ServerSentEventOfstring": {
            "type": "object",
            "title": "ServerSentEventOfstring"
        },
        "TokenCredential": {
            "type": "object",
            "properties": {
                "valur": {
                    "type": "string"
                }
            },
            "title": "TokenCredential"
        }
    }
}