{
    "swagger": "2.0",
    "info": {
        "version": "0.0.1",
        "title": "Apollo Portal Backend"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "tags": [{
        "name": "login-controller",
        "description": "Login Controller"
    }],
    "consumes": ["application/json"],
    "produces": ["application/json"],
    "paths": {
        "/login": {
            "post": {
                "tags": ["login-controller"],
                "summary": "authenticate",
                "operationId": "authenticateUsingPOST",
                "parameters": [{
                    "in": "body",
                    "name": "authentication",
                    "description": "authentication",
                    "required": true,
                    "schema": {
                        "$ref": "#/definitions/Authentication"
                    }
                }],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/MonoOfVoid"
                        }
                    },
                    "201": {
                        "description": "Created"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                },
                "deprecated": false
            }
        }
    },
    "securityDefinitions": {
        "oauth2": {
            "type": "oauth2",
            "tokenUrl": "http://apollo:8080/auth",
            "flow": "password",
            "scopes": {
                "Apoollo": "Apoollo API"
            }
        }
    },
    "definitions": {
        "Authentication": {
            "type": "object",
            "properties": {
                "password": {
                    "type": "string"
                },
                "useranme": {
                    "type": "string"
                }
            },
            "title": "Authentication"
        },
        "MonoOfVoid": {
            "type": "object",
            "title": "MonoOfVoid"
        }
    }
}